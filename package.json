{
  "name": "parrot-mocker-web",
  "version": "1.5.4",
  "description": "Retransmit requests to real servers or just mock",
  "main": "./server/index.js",
  "scripts": {
    "build": "webpack",
    "start": "node ./server/index.js",
    "dev": "NODE_ENV=dev webpack-dev-server",
    "deploy:local": "node ./server/index.js > log.txt 2>&1 &",
    "deploy:lean": "lean deploy",
    "deploy:now": "now --prod --public --confirm --token=$NOW_TOKEN && now remove parrot-mocker-web -s -f -y --token=$NOW_TOKEN",
    "test": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": ">=6.x"
  },
  "now": {
    "version": 2,
    "alias": "parrotmocker"
  },
  "keywords": [
    "api",
    "mock"
  ],
  "author": "Xianming Zhong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chinesedfan/parrot-mocker-web.git"
  },
  "homepage": "https://github.com/chinesedfan/parrot-mocker-web",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-component": "^0.10.1",
    "babel-preset-stage-0": "^6.22.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.26.1",
    "file-loader": "^1.1.5",
    "jest": "^23.0.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "supertest": "^3.1.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^10.2.1",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "co": "^4.6.0",
    "co-body": "^4.2.0",
    "element-ui": "^2.0.8",
    "jsoneditor.webapp": "^1.0.0",
    "kcors": "^1.3.2",
    "koa": "^1.2.5",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-send": "^3.3.0",
    "koa-static": "^2.1.0",
    "lodash": "^4.16.4",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^1.6.3",
    "pem": "^1.9.7",
    "qs": "^6.3.1",
    "socket.io": "^1.7.3",
    "tiny-cookie": "^1.0.1",
    "vue": "^2.1.10",
    "vue2-qrcode": "^1.0.0",
    "vuex": "^2.1.1"
  }
}
